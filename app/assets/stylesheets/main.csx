@page {
   size: 297mm 210mm;
   margin: 0mm 0mm 0mm 0mm;
}

@media print
   {
      .noprint { display: none; }
   }

table, th, td {
    border: 1px solid black;
    border-collapse: collapse;
    font-size:12px;
}

td {
    height: 25px;
}

th {
    height: 25px;
    background-color: green;
    color: white;
}

.bg { background-color: #f7fb02; width: 100%; height: 100%; display: block; }